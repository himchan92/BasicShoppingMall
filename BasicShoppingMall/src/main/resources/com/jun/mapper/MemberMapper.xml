<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jun.mapper.MemberMapper">

	<insert id="memberJoin">
		INSERT INTO BOOK_MEMBER
		VALUES
		(
			#{memberId},
			#{memberPW},
			#{memberName},
			#{memberMail},
			#{memberAddr1},
			#{memberAddr2},
			#{memberAddr3},
			0,
			sysdate,
			100000,
			5000
		)		
	</insert>

</mapper>